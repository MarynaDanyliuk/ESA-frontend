{
  "builds": [
    {
      "src": "src/*.html",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/$1", "status": 200 }
  ],
  "env": {
    "GITHUB_TOKEN": "@github-token"
  },
  "github": {
    "autoAlias": true,
    "silent": true
  },
  "alias": {
    "www.export-service-agency.netlify.app": "export-service-agency.vercel.app"
  },
  "project": "export-service-agency"
}
